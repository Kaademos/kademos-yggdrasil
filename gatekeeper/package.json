{
  "name": "@yggdrasil/gatekeeper",
  "version": "1.0.0",
  "description": "Yggdrasil Gatekeeper - Reverse proxy and progression enforcement",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc",
    "build:frontend": "cd frontend && vite build",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:frontend": "cd frontend && vite",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "security",
    "ctf",
    "reverse-proxy"
  ],
  "author": "Project Yggdrasil",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^2.0.6",
    "prom-client": "^15.1.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "winston": "^3.19.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/csurf": "^1.11.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/supertest": "^6.0.3",
    "@types/three": "^0.182.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "postcss": "^8.5.6",
    "prettier": "^3.1.1",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.17",
    "three": "^0.182.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^7.2.7"
  }
}
